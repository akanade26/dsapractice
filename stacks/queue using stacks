class MyQueue {
    stack <int> s1;
    stack <int> s2;
public:
    MyQueue() {
        
        
    }
    
    void push(int x) {
        s1.push(x);
        
        
    }
    
    int pop() {
        if(!s2.empty())
        {
            int ans = s2.top();
            s2.pop();
            return ans;
        }
        else
        {
            while(!s1.empty())
            {
                s2.push(s1.top());
                s1.pop();
            }
            int ans = s2.top();
            s2.pop();
            return ans;
        }
        
    }
    
    int peek() {
        if(!s2.empty())
        {
            return s2.top();

        }
        else
        {
            while(!s1.empty())
            {
                s2.push(s1.top());
                s1.pop();
            }
            return s2.top();
        }
        
    }
    
    bool empty() {
        if(s1.empty() && s2.empty())
        {
            return true;
        }
        else
        {
            return false;
        }
    }
};
